lassName:\"about-block\",children:[/*#__PURE__*/_jsx(\"h3\",{className:\"about-subtitle\",children:\"\\u041D\\u0430\\u0448\\u0430 \\u043A\\u043E\\u043C\\u0430\\u043D\\u0434\\u0430\"}),/*#__PURE__*/_jsx(\"p\",{className:\"about-text\",children:\"\\u0423 \\u043D\\u0430\\u0448\\u0456\\u0439 \\u043A\\u043E\\u043C\\u0430\\u043D\\u0434\\u0456 \\u043F\\u0440\\u0430\\u0446\\u044E\\u044E\\u0442\\u044C \\u0441\\u043F\\u0440\\u0430\\u0432\\u0436\\u043D\\u0456 \\u043F\\u0440\\u043E\\u0444\\u0435\\u0441\\u0456\\u043E\\u043D\\u0430\\u043B\\u0438 \\u2014 \\u043C\\u0435\\u043D\\u0435\\u0434\\u0436\\u0435\\u0440\\u0438, \\u0433\\u0456\\u0434\\u0438, \\u043A\\u043E\\u043E\\u0440\\u0434\\u0438\\u043D\\u0430\\u0442\\u043E\\u0440\\u0438 \\u0442\\u0430 \\u0441\\u043B\\u0443\\u0436\\u0431\\u0430 \\u043F\\u0456\\u0434\\u0442\\u0440\\u0438\\u043C\\u043A\\u0438. \\u041C\\u0438 \\u2014 \\u043C\\u0430\\u043D\\u0434\\u0440\\u0456\\u0432\\u043D\\u0438\\u043A\\u0438 \\u0432 \\u0434\\u0443\\u0448\\u0456, \\u044F\\u043A\\u0456 \\u0437\\u043D\\u0430\\u044E\\u0442\\u044C, \\u044F\\u043A \\u0437\\u0440\\u043E\\u0431\\u0438\\u0442\\u0438 \\u0432\\u0430\\u0448 \\u0432\\u0456\\u0434\\u043F\\u043E\\u0447\\u0438\\u043D\\u043E\\u043A \\u0456\\u0434\\u0435\\u0430\\u043B\\u044C\\u043D\\u0438\\u043C \\u0456 \\u0431\\u0435\\u0437\\u0442\\u0443\\u0440\\u0431\\u043E\\u0442\\u043D\\u0438\\u043C.\"})]}),/*#__PURE__*/_jsxs(\"div\",{className:\"about-block\",children:[/*#__PURE__*/_jsx(\"h3\",{className:\"about-subtitle\",children:\"\\u0412\\u0456\\u0434\\u0433\\u0443\\u043A\\u0438 \\u043D\\u0430\\u0448\\u0438\\u0445 \\u043A\\u043B\\u0456\\u0454\\u043D\\u0442\\u0456\\u0432\"}),/*#__PURE__*/_jsx(Testimonial,{})]})]})})]});};export default About;","map":{"version":3,"names":["React","CommonSection","Container","Row","Col","Testimonial","maleTourist","About"],"sources":["C:/Users/Valentin/Desktop/tour-management/frontend/src/pages/About.jsx"],"sourcesContent":["import React from 'react';\r\nimport CommonSection from '../shared/CommonSection';\r\nimport { Container, Row, Col } from 'reactstrap';\r\nimport Testimonial from '../components/Testimonial/Testimonial';\r\n\r\nimport '../styles/about.css';\r\nimport maleTourist from '../assets/images/male-tourist.png';\r\n\r\nconst About = () => {\r\n  return (\r\n    <>\r\n      <CommonSection title=\"Про нас\" />\r\n\r\n      <section className=\"about-section\">\r\n        <Container>\r\n\r\n          {/* Блок 1: Вступ + картинка */}\r\n          <div className=\"about-block about-block--image-text\">\r\n            <Row className=\"align-items-center\">\r\n              <Col lg=\"6\">\r\n                <h2 className=\"about-title\">Ласкаво просимо до нашої туристичної компанії</h2>\r\n                <p className=\"about-text\">\r\n                  Наша компанія — це команда ентузіастів, які щодня працюють над тим, щоб дарувати людям яскраві емоції та незабутні спогади.\r\n                  Ми організовуємо подорожі у понад 50 країн світу. Кожен наш тур — це результат ретельного планування, турботи про деталі та любові до подорожей.\r\n                </p>\r\n                <p className=\"about-text\">\r\n                  Незалежно від того, чи ви мрієте про екзотичний відпочинок, культурну екскурсію чи активні пригоди — ми знайдемо тур саме для вас.\r\n                </p>\r\n              </Col>\r\n              <Col lg=\"6\">\r\n                <img src={maleTourist} alt=\"About us\" className=\"about-img\" />\r\n              </Col>\r\n            </Row>\r\n          </div>\r\n\r\n          {/* Блок 2: Місія */}\r\n          <div className=\"about-block\">\r\n            <h3 className=\"about-subtitle\">Наша місія</h3>\r\n            <p className=\"about-text\">\r\n              Ми віримо, що подорожі змінюють людей, розширюють світогляд і дарують нові горизонти. Наша місія — зробити подорожі доступними, зручними та надихаючими для кожного.\r\n            </p>\r\n          </div>\r\n\r\n          {/* Блок 3: Переваги */}\r\n          <div className=\"about-block\">\r\n            <h3 className=\"about-subtitle\">Наші переваги</h3>\r\n            <Row>\r\n              <Col lg=\"6\">\r\n                <ul className=\"about-list\">\r\n                  <li>👥 Індивідуальний підхід до кожного клієнта</li>\r\n                  <li>🌍 Широкий вибір турів на будь-який смак і бюджет</li>\r\n                  <li>📞 Цілодобова підтримка туристів</li>\r\n                  <li>🌟 Досвідчені менеджери з туристичним досвідом</li>\r\n                </ul>\r\n              </Col>\r\n              <Col lg=\"6\">\r\n                <ul className=\"about-list\">\r\n                  <li>💳 Гнучка система бронювання та оплати</li>\r\n                  <li>🤝 Співпраця з перевіреними партнерами</li>\r\n                  <li>🔐 Надійність та прозорість у процесах</li>\r\n                  <li>💬 Високий рівень задоволеності клієнтів</li>\r\n                </ul>\r\n              </Col>\r\n            </Row>\r\n          </div>\r\n\r\n          {/* Блок 4: Команда */}\r\n          <div className=\"about-block\">\r\n            <h3 className=\"about-subtitle\">Наша команда</h3>\r\n            <p className=\"about-text\">\r\n              У нашій команді працюють справжні професіонали — менеджери, гіди, координатори та служба підтримки.\r\n              Ми — мандрівники в душі, які знають, як зробити ваш відпочинок ідеальним і безтурботним.\r\n            </p>\r\n          </div>\r\n\r\n          {/* Блок 5: Відгуки */}\r\n          <div className=\"about-block\">\r\n            <h3 className=\"about-subtitle\">Відгуки наших клієнтів</h3>\r\n            <Testimonial />\r\n          </div>\r\n\r\n        </Container>\r\n      </section>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default About;\r\n"],"mappings":"AAAA,MAAOA,MAAK,KAAM,OAAO,CACzB,MAAOC,cAAa,KAAM,yBAAyB,CACnD,OAASC,SAAS,CAAEC,GAAG,CAAEC,GAAG,KAAQ,YAAY,CAChD,MAAOC,YAAW,KAAM,uCAAuC,CAE/D,MAAO,qBAAqB,CAC5B,MAAOC,YAAW,KAAM,mCAAmC,CAAC,6IAE5D,GAAMC,MAAK,CAAG,QAARA,M