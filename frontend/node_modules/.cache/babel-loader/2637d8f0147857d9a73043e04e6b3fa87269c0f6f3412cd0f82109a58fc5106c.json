/shared/CommonSection';\r\nimport { Container, Row, Col } from 'reactstrap';\r\nimport Testimonial from '../components/Testimonial/Testimonial';\r\n\r\nimport '../styles/about.css';\r\nimport maleTourist from '../assets/images/male-tourist.png';\r\n\r\nconst About = () => {\r\n  return (\r\n    <>\r\n      <CommonSection title=\"–ü—Ä–æ –Ω–∞—Å\" />\r\n\r\n      <section className=\"about-section\">\r\n        <Container>\r\n\r\n          {/* –ë–ª–æ–∫ 1: –í—Å—Ç—É–ø + –∫–∞—Ä—Ç–∏–Ω–∫–∞ */}\r\n          <div className=\"about-block about-block--image-text\">\r\n            <Row className=\"align-items-center\">\r\n              <Col lg=\"6\">\r\n                <h2 className=\"about-title\">–õ–∞—Å–∫–∞–≤–æ –ø—Ä–æ—Å–∏–º–æ –¥–æ –Ω–∞—à–æ—ó —Ç—É—Ä–∏—Å—Ç–∏—á–Ω–æ—ó –∫–æ–º–ø–∞–Ω—ñ—ó</h2>\r\n                <p className=\"about-text\">\r\n                  –ù–∞—à–∞ –∫–æ–º–ø–∞–Ω—ñ—è ‚Äî —Ü–µ –∫–æ–º–∞–Ω–¥–∞ –µ–Ω—Ç—É–∑—ñ–∞—Å—Ç—ñ–≤, —è–∫—ñ —â–æ–¥–Ω—è –ø—Ä–∞—Ü—é—é—Ç—å –Ω–∞–¥ —Ç–∏–º, —â–æ–± –¥–∞—Ä—É–≤–∞—Ç–∏ –ª—é–¥—è–º —è—Å–∫—Ä–∞–≤—ñ –µ–º–æ—Ü—ñ—ó —Ç–∞ –Ω–µ–∑–∞–±—É—Ç–Ω—ñ —Å–ø–æ–≥–∞–¥–∏.\r\n                  –ú–∏ –æ—Ä–≥–∞–Ω—ñ–∑–æ–≤—É—î–º–æ –ø–æ–¥–æ—Ä–æ–∂—ñ —É –ø–æ–Ω–∞–¥ 50 –∫—Ä–∞—ó–Ω —Å–≤—ñ—Ç—É. –ö–æ–∂–µ–Ω –Ω–∞—à —Ç—É—Ä ‚Äî —Ü–µ —Ä–µ–∑—É–ª—å—Ç–∞—Ç —Ä–µ—Ç–µ–ª—å–Ω–æ–≥–æ –ø–ª–∞–Ω—É–≤–∞–Ω–Ω—è, —Ç—É—Ä–±–æ—Ç–∏ –ø—Ä–æ –¥–µ—Ç–∞–ª—ñ —Ç–∞ –ª—é–±–æ–≤—ñ –¥–æ –ø–æ–¥–æ—Ä–æ–∂–µ–π.\r\n                </p>\r\n                <p className=\"about-text\">\r\n                  –ù–µ–∑–∞–ª–µ–∂–Ω–æ –≤—ñ–¥ —Ç–æ–≥–æ, —á–∏ –≤–∏ –º—Ä—ñ—î—Ç–µ –ø—Ä–æ –µ–∫–∑–æ—Ç–∏—á–Ω–∏–π –≤—ñ–¥–ø–æ—á–∏–Ω–æ–∫, –∫—É–ª—å—Ç—É—Ä–Ω—É –µ–∫—Å–∫—É—Ä—Å—ñ—é —á–∏ –∞–∫—Ç–∏–≤–Ω—ñ –ø—Ä–∏–≥–æ–¥–∏ ‚Äî –º–∏ –∑–Ω–∞–π–¥–µ–º–æ —Ç—É—Ä —Å–∞–º–µ –¥–ª—è –≤–∞—Å.\r\n                </p>\r\n              </Col>\r\n              <Col lg=\"6\">\r\n                <img src={maleTourist} alt=\"About us\" className=\"about-img\" />\r\n              </Col>\r\n            </Row>\r\n          </div>\r\n\r\n          {/* –ë–ª–æ–∫ 2: –ú—ñ—Å—ñ—è */}\r\n          <div clas